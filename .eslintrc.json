{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "globals": {
    "process": true,
    "console": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": "off",
    "no-extra-semi": "off",
    "no-unused-vars": "warn",
    "no-unreachable": "warn",
    "no-redeclare": "off",
    "no-inner-declarations": "warn",
    "no-empty": "warn"
  }
}
