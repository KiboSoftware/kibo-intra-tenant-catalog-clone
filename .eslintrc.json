{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "globals": {
    "process": true,
    "console": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": "off",
    "no-extra-semi": "off",
    "no-unused-vars":"warn"
  }
}
